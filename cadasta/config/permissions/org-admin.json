{
  "clause": [
    {
      "effect": "allow",
      "action": ["org.*", "org.*.*", "project.*", "project.*.*"]
      "object": ["organization/$organization"],
    },

    {
      "effect": "allow",
      "action": ["project.*", "project.*.*"]
      "object": ["project/$organization/*"],
    }
  ]
}
