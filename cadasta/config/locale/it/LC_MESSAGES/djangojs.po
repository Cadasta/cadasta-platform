#: core/static/js/parsley.js:2282
msgid "This field seems to be invalid."
msgstr "Questo campo sembra invalido."

#: core/static/js/parsley.js:2284
msgid "This field should be a valid email."
msgstr "Questo campo dovrebbe avere un'email valida."

#: core/static/js/parsley.js:2285
msgid "This value should be a valid url."
msgstr "Questo valore dovrebbe essere un valido url."

#: core/static/js/parsley.js:2286
msgid "This value should be a valid number."
msgstr "Questo valore dovrebbe essere un numero valido."

#: core/static/js/parsley.js:2287
msgid "This value should be a valid integer."
msgstr "Questo valore dovrebbe essere un valido numero intero."

#: core/static/js/parsley.js:2288
msgid "This value should be digits."
msgstr "Questo valore dovrebbe essere numerico."

#: core/static/js/parsley.js:2289
msgid "This value should be alphanumeric."
msgstr "Questo valore dovrebbe essere alfanumerico."

#: core/static/js/parsley.js:2291
msgid "This field should not be blank."
msgstr "Questo campo non dovrebbe essere vuoto."

#: core/static/js/parsley.js:2292
msgid "This field is required."
msgstr "Questo campo è obbligatorio."

#: core/static/js/parsley.js:2293
msgid "This value seems to be invalid."
msgstr "Questo valore sembra essere invalido."

#: core/static/js/parsley.js:2294
#, javascript-format
msgid "This value should be greater than or equal to %s."
msgstr "Questo valore dovrebbe essere maggiore o uguale a %s."

#: core/static/js/parsley.js:2295
#, javascript-format
msgid "This value should be lower than or equal to %s."
msgstr "Questo valore dovrebbe essere minore o uguale a %s."

#: core/static/js/parsley.js:2296
#, javascript-format
msgid "This value should be between %s and %s."
msgstr "Questo valore dovrebbe essere tra %s e %s."

#: core/static/js/parsley.js:2297
#, javascript-format
msgid "This field is too short. It should have %s characters or more."
msgstr "Questo campo è troppo corto. Dovrebbe avere almeno %s caratteri."

#: core/static/js/parsley.js:2298
#, javascript-format
msgid "This field is too long. It should have %s characters or fewer."
msgstr "Questo campo è troppo lungo. Non dovrebbe avere più di %s caratteri."

#: core/static/js/parsley.js:2299
#, javascript-format
msgid ""
"This field length is invalid. It should be between %s and %s characters "
"long."
msgstr ""
"La lunghezza del campo è invalida. Dovrebbe avere tra %s e %s caratteri."

#: core/static/js/parsley.js:2300
#, javascript-format
msgid "You must select at least %s choices."
msgstr "Devi selezionare almeno %s opzioni."

#: core/static/js/parsley.js:2301
#, javascript-format
msgid "You must select %s choices or fewer."
msgstr "Devi selezionare non più di %s opzioni."

#: core/static/js/parsley.js:2302
#, javascript-format
msgid "You must select between %s and %s choices."
msgstr "Devi selezionare tra %s e %s opzioni."

#: core/static/js/parsley.js:2303
msgid "This value should be the same."
msgstr "Questo valore dovrebbe essere lo stesso."

#: core/static/js/parsleyAddValidator.js:16
msgid ""
"Your password must contain at least 3 of the following: lowercase "
"characters, uppercase characters, special characters, and/or numerical "
"characters."
msgstr ""
"La tua password deve contenere almeno 3 dei seguenti: caratteri minuscoli, "
"caratteri maiuscoli, caratteri speciali, e/o caratteri numerici."

#: core/static/js/parsleyAddValidator.js:26
msgid "Your password cannot contain your username."
msgstr "La tua password non può contenere il tuo nome utente."

#: core/static/js/parsleyAddValidator.js:36
msgid "Your password cannot contain your email mailbox name."
msgstr "La tua password non può contenere il nome della tua posta email."
